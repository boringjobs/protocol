    "verify": "cannon verify boringjobs:latest@main --api-key $ETHERSCAN_API_KEY --chain-id $CHAIN_ID"
  },
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@usecannon/cli": "^2.11.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}