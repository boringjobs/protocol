{
  "name": "boringjobs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "forge test",
    "inspect": "cannon inspect boringjobs --write-deployments ./deployments",
    "build-cannonfile": "cannon build cannonfile.toml",
    "start": "cannon boringjobs",
    "dev": "cannon boringjobs --registry-priority local",
